    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>var move = 0;
keyUp = keyboard_check_pressed(vk_up);
keyDown = keyboard_check_pressed(vk_down);
keyEnter = keyboard_check_pressed(vk_enter);
keyZ = keyboard_check_pressed(ord("Z"));
keyX = keyboard_check_pressed(ord("X"));

move -= max(keyUp, 0);
move += max(keyDown, 0);

//Move options around
if (obj_menu.menuState != 0){
    if (move != 0){
        opos += move;
        audio_play_sound(snd_select, 1, false);
        audio_sound_gain(snd_select, global.gameVolume/10, false);//Since this sound is annoyingly loud
        //Wrap options position around
        if (opos &lt; 0){
            opos = array_length_1d(options) - 1;
        }
        if (opos &gt; array_length_1d(options) - 1){
            opos = 0;
        }
    }
    
    //Select an option
    var select;
    select = max(keyEnter, keyZ, 0);
    if (select == 1){
        scr_options();
        audio_play_sound(snd_pause, 1, false);
        audio_sound_gain(snd_pause, global.gameVolume/10, false);
    }
    
    if (keyX){
        opos = 2;
        audio_play_sound(snd_cancel, 1, false);
        audio_sound_gain(snd_cancel, global.gameVolume/10, false);//Since this sound is annoyingly loud
    }
}
</string>
          </argument>
        </arguments>
      </action>
    </event>
    <event eventtype="8" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
