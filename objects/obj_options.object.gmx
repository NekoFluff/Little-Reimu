        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Draw Options
draw_set_halign(fa_left);
draw_set_valign(fa_middle);
draw_set_font(fnt_menu);
draw_set_color(c_white);

//If Options is selected
if (obj_menu.menuState == 1){
    desiredX = 16;
    for (i = 0; i &lt; array_length_1d(options); i++){
        positions[i] = positions[i] + max(min(0.5 * (desiredX - positions[i]), 25), -25) 
        if (i == opos){ // Current selected option
            draw_set_font(fnt_selected);
            draw_set_alpha(1);
            //Outline text
            draw_set_color(c_black);
            draw_text(positions[i]-1, 260 + (i * space), string(options[i]));
            draw_text(positions[i]+1, 260 + (i * space), string(options[i]));
            draw_text(positions[i], 260+1 + (i * space), string(options[i]));
            draw_text(positions[i], 260-1 + (i * space), string(options[i]));
            //Actual text
            draw_set_color(c_white);
            draw_text(positions[i], 260 + (i * space), string(options[i]));
        } else { // Not selected options
            draw_set_font(fnt_menu);
            draw_set_alpha(0.6);
            //Outline text
            draw_set_color(c_black);
            draw_text(positions[i]+1, 264 + (i * space), string(options[i]));
            draw_text(positions[i]-1, 264 + (i * space), string(options[i]));
            draw_text(positions[i], 264+1 + (i * space), string(options[i]));
            draw_text(positions[i], 264-1 + (i * space), string(options[i]));
            //Actual text
            draw_set_color(c_white);
            draw_text(positions[i], 264 + (i * space), string(options[i]));
        }
    }
} else { //If Options not selected
    desiredX = -260;
    for (i = 0; i &lt; array_length_1d(options); i++){
        positions[i] = positions[i] + max(min(0.35 * (desiredX - positions[i]), 15), -15) 
        draw_set_font(fnt_menu);
        draw_set_alpha(0.6);
        //Outline text
        draw_set_color(c_black);
        draw_text(positions[i]+1, 264 + (i * space), string(options[i]));
        draw_text(positions[i]-1, 264 + (i * space), string(options[i]));
        draw_text(positions[i], 264+1 + (i * space), string(options[i]));
        draw_text(positions[i], 264-1 + (i * space), string(options[i]));
        //Actual text
        draw_set_color(c_white);
        draw_text(positions[i], 264 + (i * space), string(options[i]));
    }
}
</string>
