for (i = 0; i &lt; array_length_1d(menu); i++){
    if (i == mpos){ // Current selected option
        draw_set_font(fnt_selected);
        draw_set_alpha(1);
        draw_text(540 + space, 140 + (i * space), string(menu[i]));
