        draw_text(positions[i], 264+1 + (i * space), string(menu[i]));
        draw_text(positions[i], 264-1 + (i * space), string(menu[i]));
        //Actual text
        draw_set_color(c_white);
        draw_text(positions[i], 264 + (i * space), string(menu[i]));
    }
}
