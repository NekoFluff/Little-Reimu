        <arguments>
          <argument>
            <kind>1</kind>
            <string>var move = 0;
keyUp = keyboard_check_pressed(vk_up);
keyDown = keyboard_check_pressed(vk_down);
keyEnter = keyboard_check_pressed(vk_enter);
keyZ = keyboard_check_pressed(ord("Z"));
keyX = keyboard_check_pressed(ord("X"));

move -= max(keyUp, 0);
move += max(keyDown, 0);

//Move menu around
if (obj_menu.menuState == 0){
    if (move != 0){
        mpos += move;
        audio_play_sound(snd_select, 1, false);
        audio_sound_gain(snd_select, global.gameVolume/10, false);//Since this sound is annoyingly loud
        //Wrap menu position around
        if (mpos &lt; 0){
            mpos = array_length_1d(menu) - 1;
        }
        if (mpos &gt; array_length_1d(menu) - 1){
            mpos = 0;
        }
    }
    
    //Select an option
    var select;
    select = max(keyEnter, keyZ, 0);
    if (select == 1){
        scr_menu();
        show_debug_message("Selected " + string(mpos));
        audio_play_sound(snd_confirm, 1, false);
        audio_sound_gain(snd_confirm, global.gameVolume/10, false);
    }
    
    if (keyX){
        mpos = 4;
        audio_play_sound(snd_cancel, 1, false);
        audio_sound_gain(snd_cancel, global.gameVolume/10, false);//Since this sound is annoyingly loud
    }
}
</string>
          </argument>
        </arguments>
      </action>
